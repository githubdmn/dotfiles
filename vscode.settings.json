{
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.autoIndent": "brackets",
  "debug.node.autoAttach": "off",
  "terminal.integrated.cursorStyle": "underline",
  "workbench.panel.defaultLocation": "right", //"right",
  // Enable usage data and errors to be sent to a Microsoft online service.
  "telemetry.telemetryLevel": "off",
  // Number of editors shown in the Open Editors pane.
  "explorer.openEditors.visible": 0,
  "git.autofetch": true,
  "terminal.integrated.fontSize": 11,
  "workbench.statusBar.visible": true,
  "window.menuBarVisibility": "toggle",
  "workbench.editor.tabCloseButton": "off",
  "breadcrumbs.enabled": false,
  "editor.formatOnPaste": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Dracula",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "vscode-icons",
  "explorer.confirmDelete": false,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.tabSize": 5,
  "editor.insertSpaces": false,
  "security.workspace.trust.untrustedFiles": "open",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.format.enable": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.enable": true,
  "javascript.format.semicolons": "remove",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript", "typescript"],
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "remote.SSH.enableRemoteCommand": true,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.suppressWindowsSshWarning": true,
  "remote.SSH.configFile": "/home/dmn/.ssh/config",
  "git.confirmSync": false,
  "settingsSync.ignoredSettings": [
    "application.shellEnvironmentResolutionTimeout"
],
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "'Droid Sans Mono Bold', 'monospace bold', monospace, 'Droid Sans Mono'",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontWeight": "bold",
  "workbench.activityBar.location": "hidden"
}
